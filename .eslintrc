{
	"extends": ["airbnb-base", "plugin:flowtype/recommended"],
	"parser": "babel-eslint",
	"plugins": ["flowtype"],
	"parserOptions": {},
	"rules": {
		"import/default": 0,
		"import/no-amd": 0,
		"import/no-mutable-exports": 0,
		"import/named": 0,
		"import/namespace": 0,
		"import/no-extraneous-dependencies": 0,
		"no-unused-vars": [
			1,
			{
				"vars": "all",
				"argsIgnorePattern": "next|success|error|app|express|render|Router|Route|Link"
			}
		],
		"comma-dangle": 0,
		"indent": 0,
		"no-console": 0,
		"no-tabs": 0,
		"max-len": [2, 2000, 4],
		"id-length": [
			2,
			{
				"min": 1,
				"max": 50,
				"properties": "never",
				"exceptions": []
			}
		],
		"no-param-reassign": [
			2,
			{
				"props": false
			}
		],
		"prefer-template": 0,
		"no-path-concat": 0,
		"class-methods-use-this": 0,
		"object-curly-spacing": 0,
		"consistent-return": 0,
		"guard-for-in": 0,
		"no-restricted-syntax": 0,
		"arrow-body-style": ["error", "as-needed"],
		"arrow-parens": ["error", "as-needed"],
		"flowtype/no-types-missing-file-annotation": 0,
		"lines-between-class-members": 0,
		"object-curly-newline": 0,
		"implicit-arrow-linebreak": 0,
		"function-paren-newline": 0,
		"prefer-destructuring": [
			"error",
			{
				"AssignmentExpression": {
					"array": false,
					"object": false
				}
			}
		],
		"prefer-rest-params": 0
	},
	"settings": {
		"import/resolver": {
			"babel-module": {}
		}
	},
	"globals": {}
}
